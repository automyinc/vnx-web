package vnx.web;

import vnx.Hash128;
import vnx.Value;
import vnx.TopicPtr;

class Request {
	
	Hash128 id;
	
	Hash128 stream;
	
	request_type_e type = GENERIC;
	
	Path path;
	
	Value* parameter;
	
	TopicPtr channel;				// where to return response
	
	long time_stamp_ms;				// time when request was sent [ms]
	
	long timeout_ms = 3000;			// how long to wait until fail [ms]
	
}
